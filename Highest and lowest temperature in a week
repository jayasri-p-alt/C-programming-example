#include <stdio.h>

int main() {
    int i;
    float temp[7], max, min;

    for (i = 0; i < 7; i++) {
        printf("Enter temperature of day %d: ", i + 1);
        scanf("%f", &temp[i]);
    }

    max = min = temp[0];

    for (i = 1; i < 7; i++) {
        if (temp[i] > max) max = temp[i];
        if (temp[i] < min) min = temp[i];
    }

    printf("Highest = %.2f\nLowest = %.2f\n", max, min);
    return 0;
}