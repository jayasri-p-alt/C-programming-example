#include <stdio.h>

int main() {
    char str[100];
    int i, j;
    char temp;

    printf("Enter a string: ");
    fgets(str, 100, stdin);


    for(i = 0; str[i] != '\0'; i++);
    j = i - 1;

    for(i = 0; i < j; i++, j--) {
        temp = str[i];
        str[i] = str[j];
        str[j] = temp;
    }

    printf("Reversed string: %s", str);
    return 0;
}
