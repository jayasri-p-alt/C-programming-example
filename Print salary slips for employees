#include <stdio.h>

int main() {
    int i, n;
    float salary;

    printf("Enter number of employees: ");
    scanf("%d", &n);

    for (i = 1; i <= n; i++) {
        printf("Enter salary of employee %d: ", i);
        scanf("%f", &salary);
        printf("Salary Slip %d: Rs. %.2f\n", i, salary);
    }
    return 0;
}