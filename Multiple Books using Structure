#include <stdio.h>

struct book {
    int id;
    char title[20];
    float price;
};

int main() {
    struct book b[10];
    int n,i;
    printf("Enter number of books: ");
    scanf("%d",&n);

    for(i=0;i<n;i++){
        scanf("%d %s %f",&b[i].id,b[i].title,&b[i].price);
    }

    for(i=0;i<n;i++){
        printf("%d %s %.2f\n",b[i].id,b[i].title,b[i].price);
    }
}