#include <stdio.h>

int main() {
    char str1[50], str2[50];
    int i = 0;

    printf("Enter first string: ");
    gets(str1);
    printf("Enter second string: ");
    gets(str2);

    while(str1[i] == str2[i] && str1[i] != '\0') {
        i++;
    }

    if(str1[i] == str2[i])
        printf("Both strings are equal");
    else
        printf("Strings are not equal");

    return 0;
}