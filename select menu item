#include <stdio.h>

int main() {
    int choice;

    printf("1. Tea\n2. Coffee\n3. Juice\n");
    printf("Enter your choice: ");
    scanf("%d", &choice);

    switch (choice) {
        case 1: printf("You selected Tea\n"); break;
        case 2: printf("You selected Coffee\n"); break;
        case 3: printf("You selected Juice\n"); break;
        default: printf("Invalid Choice\n");
    }

    return 0;
}