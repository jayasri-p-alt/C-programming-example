#include <stdio.h>

int main() {
    int n,a[10],b[10],i;
    scanf("%d",&n);

    for(i=0;i<n;i++)
        scanf("%d",&a[i]);

    for(i=0;i<n;i++)
        b[i]=a[i];

    printf("Array A:\n");
    for(i=0;i<n;i++) printf("%d ",a[i]);

    printf("\nArray B:\n");
    for(i=0;i<n;i++) printf("%d ",b[i]);
}