#include <stdio.h>

int main() {
    int i, n;
    float marks, total = 0;

    printf("Enter number of students: ");
    scanf("%d", &n);

    for (i = 1; i <= n; i++) {
        printf("Enter marks of student %d: ", i);
        scanf("%f", &marks);
        total += marks;
    }

    printf("Total Marks = %.2f\n", total);

    return 0;
}