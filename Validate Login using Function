#include <stdio.h>
#include <string.h>

int login(char u[], char p[]) {
    if (strcmp(u, "admin") == 0 && strcmp(p, "1234") == 0)
        return 1;
    return 0;
}

int main() {
    char u[20], p[20];
    scanf("%s %s", u, p);
    if (login(u, p)) printf("Login Success");
    else printf("Invalid Login");
}