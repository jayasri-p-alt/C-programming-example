#include <stdio.h>

int main() {
    int i, n;
    float price[50], key;

    printf("Enter number of items: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        printf("Enter price of item %d: ", i + 1);
        scanf("%f", &price[i]);
    }

    printf("Enter price to search: ");
    scanf("%f", &key);

    for (i = 0; i < n; i++) {
        if (price[i] == key) {
            printf("Price found at position %d\n", i + 1);
            return 0;
        }
    }
    printf("Price not found\n");
    return 0;
}