#include <stdio.h>

int main() {
    int i, n;
    float price, total = 0;
    int quantity;

    printf("Enter number of items: ");
    scanf("%d", &n);

    for (i = 1; i <= n; i++) {
        printf("\nEnter price of item %d: ", i);
        scanf("%f", &price);

        printf("Enter quantity of item %d: ", i);
        scanf("%d", &quantity);

        total = total + (price * quantity);
    }

    printf("\nTotal Bill Amount = Rs. %.2f\n", total);

    return 0;
}
